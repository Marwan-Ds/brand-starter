generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model BrandKit {
  id        String   @id @default(cuid())
  userId    String
  createdAt DateTime @default(now())
  mode      String
  business  String
  vibe      String
  kitJson   Json

  @@index([userId])
}
